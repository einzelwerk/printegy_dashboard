.dashboard__products__categoriesblock
    block sidebar_btn
        button(type="button").btn.-lightblue.-medium.-wplus.-full New Product
            <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M3 8H13" stroke="#0E86F4" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M8 3V13" stroke="#0E86F4" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
    .dashboard__products__categories
        .dashboard__products__category.toggle__elem
            .dashboard__products__category-head.toggle__elem__btn.toggle__arrow
                .title Designs
                <svg class="icon" width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M11.375 5.25L7 9.625L2.625 5.25" stroke="#AAADB3" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            .dashboard__products__category-content.toggle__elem__content
                ul.category__list
                    li.category__list-item
                        a(href="") All
                            span.count (7)
                    li.category__list-item
                        a(href="") Apparel
                            span.count (4)
        .dashboard__products__category.active.toggle__elem
            .dashboard__products__category-head.toggle__elem__btn.toggle__arrow
                .title Categories
                <svg class="icon" width="12" height="7" viewBox="0 0 12 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M10.375 5.75L6 1.375L1.625 5.75" stroke="#797A80" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            .dashboard__products__category-content.toggle__elem__content--active
                ul.category__list
                    li.category__list-item.active
                        a(href="") All
                            span.count (7)
                    li.category__list-item
                        a(href="") Apparel
                            span.count (4)
                    li.category__list-item
                        a(href="") Homeware
                            span.count (1)
                    li.category__list-item
