.dashboard__product__popup-content__review__sideblock.back-grey.pricepage
    .tabs
        a(href="").tab Product Design
        a(href="").tab Settings & Variants
        a(href="").tab.active Shipping & Pricing

    .dashboard__product__popup-content__shipping-pricing__table__wrapper.back-white
        p.desc.p13-400 Shipping
        .dashboard__product__popup-content__shipping-pricing__table
            .table__item
                .name Destination
                .cselect#contacts-theme-country.dr__el
                    select#filter-select-country(name="country" style="display:none")
                        option(value='en')
                        option(value='de')
                        option(value='ua')
                    .__title.dr__el-btn
                        span USA
                    .__content.dr__el-block
                        div(data-value="en").cselect__label
                            span USA
                        div(data-value="de").cselect__label
                            span Germany
                        div(data-value="de").cselect__label
                            span Ukraine
            .table__item
                .name Mail Courier
                .value USPS
            .table__item
                .name Cost (ex GST 20%)
                .value €4.79
            .table__item
                .name Additional piece cost (ex GST 20%)
                .value €1.74
            .table__item
                .name Tracked
                .value Yes
            .table__item
                .name Delivery Time
                .value 7-11 days

    .dashboard__product__popup-content__shipping-pricing__table__wrapper.back-grey

        .dashboard__product__popup-content__header.pricetable__event
            .desc Pricing
            .pricetablebtn
                <svg class="icon" width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
